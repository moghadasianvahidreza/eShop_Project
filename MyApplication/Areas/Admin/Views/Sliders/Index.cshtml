@model IEnumerable<Models.Slider>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
	<tr>
		<th>
			@Html.DisplayNameFor(model => model.Tiltle)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.StartDate)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.EndDate)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.IsActive)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Url)
		</th>
		<th></th>
	</tr>

@foreach (var item in Model) {
<tr>
	<td>
		@Html.DisplayFor(modelItem => item.Tiltle)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.StartDate)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.EndDate)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.IsActive)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.Url)
	</td>
	<td>
		@Html.ActionLink("Edit", "Edit", new { id = item.SlideId }) |
		@Html.ActionLink("Details", "Details", new { id = item.SlideId }) |
		@Html.ActionLink("Delete", "Delete", new { id = item.SlideId })
	</td>
</tr>
}

</table>
