@model IEnumerable<Models.Product>

@{
	string title = "صفحه جستجوی محصولات";
	ViewBag.Title = title;
}

<h2>
	@title
</h2>

<hr />

<section class="last-products">
	<header><h3>تعداد @Model.Count() محصول با کلمه @ViewBag.search یافت شد.</h3> </header>
	<article>

		@foreach (var item in Model)
		{
			@Html.Partial("~/Views/Shared/PartialViews/_Product.cshtml", item)
		}

		<div class="clearfix"></div>
	</article>
</section>



